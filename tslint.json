{
    "extends": ["tslint:latest", "tslint-config-prettier"],
    "rules": {
        "ordered-imports": false,
        "interface-name": [true, "never-prefix"],
        "no-implicit-dependencies": [true, "dev"],
        "max-line-length": [true, { "limit": 120, "ignore-pattern": "^import |^export {(.*?)}" }]
    }
}
